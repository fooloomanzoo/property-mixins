{
  "name": "@fooloomanzoo/property-mixins",
  "version": "2.5.1",
  "description": "mixin for custom elements to extends property mixins for data formats",
  "scripts": {
    "prepublish": "npm run build:demo && git add . && git commit -m \"publish new version\" && npm version patch && git push",
    "build:demo": "polymer analyze -i datetime-mixin.html color-mixin.html > analysis.json",
    "start": "polymer serve -o",
    "test": "polymer test",
    "test:p": "polymer test -p --config-file=wct.p.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fooloomanzoo/property-mixins.git"
  },
  "keywords": [
    "mixin",
    "polymer",
    "customElements",
    "properties",
    "date",
    "time",
    "range",
    "color"
  ],
  "author": "fooloomanzoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fooloomanzoo/property-mixins/issues"
  },
  "devDependencies": {
    "polymer-cli": "^1.7.7"
  }
}
